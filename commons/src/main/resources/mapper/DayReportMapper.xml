<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.juhai.commons.mapper.DayReportMapper">

    <resultMap id="BaseResultMap" type="com.juhai.commons.entity.DayReport">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="today" column="today" jdbcType="VARCHAR"/>
            <result property="depositAmount" column="deposit_amount" jdbcType="DECIMAL"/>
            <result property="withdrawAmount" column="withdraw_amount" jdbcType="DECIMAL"/>
            <result property="investmentAmount" column="investment_amount" jdbcType="DECIMAL"/>
            <result property="incomeAmount" column="income_amount" jdbcType="DECIMAL"/>
            <result property="registerCount" column="register_count" jdbcType="INTEGER"/>
            <result property="depositCount" column="deposit_count" jdbcType="INTEGER"/>
            <result property="withdrawCount" column="withdraw_count" jdbcType="INTEGER"/>
            <result property="investmentCount" column="investment_count" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,today,deposit_amount,
        withdraw_amount,investment_amount,income_amount,
        register_count,deposit_count,withdraw_count,
        investment_count
    </sql>
</mapper>
